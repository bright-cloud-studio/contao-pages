{
    "name": "bright-cloud-studio/contao-pages",
    "description": "Enhance Pages by adding Page Image and Page Teaser fields alongside a Page List module.",
    "keywords": ["contao", "page", "image", "list", "bright", "cloud", "studio"],
    "type": "contao-bundle",
    "homepage": "http://brightcloudstudio.com/",
    "license": "LGPL-3.0+",
    "authors":[
        {
            "name": "Bright Cloud Studio",
            "homepage": "https://brightcloudstudio.com",
            "email": "mark@brightcloudstudio.com",
            "role": "Developer"
        }
    ],
    "support":{
        "issues": "https://github.com/bright-cloud-studio/contao-pages/issues",
        "source": "https://github.com/bright-cloud-studio/contao-pages"
    },
    "require":{
        "php": "^8.1",
        "contao/core-bundle": "^5.3"
    },
    "extra":{
        "contao-manager-plugin": "Bcs\\PageBundle\\ContaoManager\\Plugin"
    },
    "autoload": {
        "psr-4": {
            "Bcs\\PageBundle\\": "src"
        },
        "classmap": [
            "src/Resources/contao"
        ],
        "exclude-from-classmap": [
            "src/Resources/contao/config/",
            "src/Resources/contao/dca/",
            "src/Resources/contao/languages/",
            "src/Resources/contao/templates/"
        ]
    }
}
